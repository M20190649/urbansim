<?xml version='1.0' encoding='utf-8'?>
<objectModel 
 xmlns='http://standards.ieee.org/IEEE1516-2010'
 xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance'
 xsi:schemaLocation='http://standards.ieee.org/IEEE1516-2010 http://standards.ieee.org/downloads/1516/1516.2-2010/IEEE1516-DIF-2010.xsd'  >
	<modelIdentification>
		<name>Sumo</name>
		<type>FOM</type>
		<version>1.0</version>
		<applicationDomain>Research</applicationDomain>
	</modelIdentification>
	<objects>
		<objectClass>
			<name>HLAobjectRoot</name>
			<objectClass>
				<name>Vehicle</name>
				<sharing>PublishSubscribe</sharing>
				<semantics>Vehicle used by SUMO for traffic simulation and by OpenSim for visual representation.</semantics>
				<attribute>
					<name>Position</name>
					<dataType>Position</dataType>
					<updateType>Conditional</updateType>
					<updateCondition>On Change</updateCondition>
					<ownership>DivestAcquire</ownership>
					<sharing>PublishSubscribe</sharing>
					<transportation>HLAreliable</transportation>
					<order>Receive</order>
					<semantics>Position of vehicle</semantics>
				</attribute>
				<attribute>
					<name>Angle</name>
					<dataType>HLAfloat64BE</dataType>
					<updateType>Conditional</updateType>
					<updateCondition>On Change</updateCondition>
					<ownership>DivestAcquire</ownership>
					<sharing>PublishSubscribe</sharing>
					<transportation>HLAreliable</transportation>
					<order>Receive</order>
					<semantics>Car angle</semantics>
				</attribute>
				<attribute>
					<name>Velocity</name>
					<dataType>HLAfloat64BE</dataType>
					<updateType>Conditional</updateType>
					<updateCondition>On Change</updateCondition>
					<ownership>DivestAcquire</ownership>
					<sharing>PublishSubscribe</sharing>
					<transportation>HLAreliable</transportation>
					<order>Receive</order>
					<semantics>Velocity of vehicle (normalized)</semantics>
				</attribute>
			</objectClass>
			<objectClass>
				<name>TrafficLight</name>
				<sharing>PublishSubscribe</sharing>
				<attribute>
					<name>ID</name>
					<dataType>HLAASCIIstring</dataType>
					<updateType>Static</updateType>
					<updateCondition>On Change</updateCondition>
					<ownership>NoTransfer</ownership>
					<sharing>PublishSubscribe</sharing>
					<transportation>HLAreliable</transportation>
					<order>TimeStamp</order>
					<semantics>N/A</semantics>
				</attribute>
				<attribute>
					<name>Status</name>
					<dataType>HLAASCIIstring</dataType>
					<updateType>Static</updateType>
					<updateCondition>On Change</updateCondition>
					<ownership>DivestAcquire</ownership>
					<sharing>PublishSubscribe</sharing>
					<transportation>HLAreliable</transportation>
					<order>TimeStamp</order>
					<semantics>N/A</semantics>
				</attribute>
				<attribute>
					<name>Position</name>
					<dataType>Position</dataType>
					<updateType>Static</updateType>
					<updateCondition>On Change</updateCondition>
					<ownership>DivestAcquire</ownership>
					<sharing>PublishSubscribe</sharing>
					<transportation>HLAreliable</transportation>
					<order>TimeStamp</order>
					<semantics>N/A</semantics>
				</attribute>
			</objectClass>
		</objectClass>
	</objects>
	<interactions>
		<interactionClass>
			<name>HLAinteractionRoot</name>
			<interactionClass>
				<name>AddVehicle</name>
				<sharing>PublishSubscribe</sharing>
				<transportation>HLAreliable</transportation>
				<order>Receive</order>
				<semantics>Creates a vehicle.</semantics>
				<parameter>
					<name>VehicleName</name>
					<dataType>HLAASCIIstring</dataType>
					<semantics>Name of vehicle</semantics>
				</parameter>
				<parameter>
					<name>VehicleType</name>
					<dataType>HLAASCIIstring</dataType>
					<semantics>Type of vehicle.</semantics>
				</parameter>
				<parameter>
					<name>DestinationName</name>
					<dataType>HLAASCIIstring</dataType>
					<semantics>Destination.</semantics>
				</parameter>
				<parameter>
					<name>Source</name>
					<dataType>HLAASCIIstring</dataType>
					<semantics>Source.</semantics>
				</parameter>
			</interactionClass>
			<interactionClass>
				<name>DeleteObject</name>
				<sharing>PublishSubscribe</sharing>
				<transportation>HLAreliable</transportation>
				<order>TimeStamp</order>
				<semantics>Destroys a simulated object.</semantics>
				<parameter>
					<name>ID</name>
					<dataType>HLAASCIIstring</dataType>
					<semantics>N/A</semantics>
				</parameter>
			</interactionClass>
			<interactionClass>
				<name>VehicleArrived</name>
				<sharing>PublishSubscribe</sharing>
				<transportation>HLAreliable</transportation>
				<order>TimeStamp</order>
				<parameter>
					<name>ID</name>
					<dataType>HLAASCIIstring</dataType>
					<semantics>N/A</semantics>
				</parameter>
				<parameter>
					<name>Position</name>
					<dataType>Position</dataType>
					<semantics>N/A</semantics>
				</parameter>
				<parameter>
					<name>VehicleType</name>
					<dataType>HLAASCIIstring</dataType>
					<semantics>N/A</semantics>
				</parameter>
			</interactionClass>
			<interactionClass>
				<name>InductionLoop</name>
				<sharing>PublishSubscribe</sharing>
				<transportation>HLAreliable</transportation>
				<order>TimeStamp</order>
				<parameter>
					<name>ID</name>
					<dataType>HLAASCIIstring</dataType>
					<semantics>N/A</semantics>
				</parameter>
				<parameter>
					<name>Count</name>
					<dataType>HLAinteger32BE</dataType>
					<semantics>N/A</semantics>
				</parameter>
			</interactionClass>
			<interactionClass>
				<name>VehicleDeparted</name>
				<sharing>PublishSubscribe</sharing>
				<transportation>HLAreliable</transportation>
				<order>TimeStamp</order>
				<parameter>
					<name>ID</name>
					<dataType>HLAASCIIstring</dataType>
					<semantics>N/A</semantics>
				</parameter>
				<parameter>
					<name>Position</name>
					<dataType>Position</dataType>
					<semantics>N/A</semantics>
				</parameter>
				<parameter>
					<name>VehicleType</name>
					<dataType>HLAASCIIstring</dataType>
					<semantics>N/A</semantics>
				</parameter>
			</interactionClass>
		</interactionClass>
	</interactions>
	<dimensions/>
	<synchronizations/>
	<transportations/>
	<updateRates/>
	<dataTypes>
		<basicDataRepresentations/>
		<simpleDataTypes/>
		<enumeratedDataTypes/>
		<arrayDataTypes/>
		<fixedRecordDataTypes>
			<fixedRecordData>
				<name>Position</name>
				<encoding>HLAfixedRecord</encoding>
				<semantics>Position in cartesian coordinates.</semantics>
				<field>
					<name>X</name>
					<dataType>HLAfloat64BE</dataType>
				</field>
				<field>
					<name>Y</name>
					<dataType>HLAfloat64BE</dataType>
				</field>
				<field>
					<name>Z</name>
					<dataType>HLAfloat64BE</dataType>
				</field>
			</fixedRecordData>
		</fixedRecordDataTypes>
		<variantRecordDataTypes/>
	</dataTypes>
	<notes/>
</objectModel>
